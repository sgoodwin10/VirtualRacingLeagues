[mysqld]
# Development optimizations for faster startup
innodb_buffer_pool_size = 32M          # Reduced from default 128M
innodb_log_file_size = 16M             # Smaller log files
innodb_flush_log_at_trx_commit = 2     # Less durable but faster
innodb_flush_method = O_DIRECT         # Better I/O performance
max_connections = 50                    # Fewer connections needed in dev
